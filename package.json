{
  "name": "swagger-egg",
  "version": "1.2.0",
  "description": "swagger document generator for egg.",
  "eggPlugin": {
    "name": "swaggerEgg"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "swagger",
    "swagger-doc",
