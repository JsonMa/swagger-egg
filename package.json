{
  "name": "egg-swagger-egg",
  "version": "1.0.0",
  "description": "swagger document generator for egg.",
  "eggPlugin": {
    "name": "swaggerEgg"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {},
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
